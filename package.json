{
  "name": "minimade",
  "version": "0.0.0",
  "description": "a minimalist skin for osu! (for real!)",
  "type": "module",
  "scripts": {
    "buildscripts": "npx tsc",
    "skin": "node --enable-source-maps scripts/index.js",
    "build": "npm run buildscripts; npm run skin",
    "clean": "npm run skin -- -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awful-coder/minimade.git"
  },
  "keywords": [
    "osu",
    "skin"
  ],
  "author": "kyle zhou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/awful-coder/minimade/issues"
  },
  "homepage": "https://github.com/awful-coder/minimade#readme",
  "dependencies": {
    "arg": "^5.0.1",
    "jsonc-parser": "^3.0.0",
    "sharp": "^0.30.3"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.23",
    "@types/sharp": "^0.30.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "typescript": "^4.6.4"
  },
  "private": true
}
