{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "semi": "warn",
    "quote-props": ["warn", "as-needed"],
    "quotes": ["warn", "double"],
    "@typescript-eslint/no-non-null-assertion": "off"
  }
}
